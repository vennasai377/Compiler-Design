%{
int flag=0 ;
%}
%%
[a-z.0-9]+@[a-z]+".com"/".in" { flag=1; }
%%
int main()
{
yylex();
if(flag==0)
printf("valid email id");
else
printf("in valid email id");
}
int yywrap()
{ return 1;
}

